{
    "configurations": [
        {
            "name": "Debug API",
            "type": "pwa-node",
            "request": "attach",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            
        },
        {
            "name": "Debug UI",
            "port": 4200,
            "request": "attach",
            "type": "pwa-chrome",
            "webRoot": "${workspaceFolder}"
        }
    ]
}